extends layout

block content
  h1= title
  - var IMG = ["pic 1.jfif", "pic 2.jpeg", "pic 3.jpg", "pic 4.jpeg", "pic 5.jpg"]
  - for (let i = 0; i < 3; i++)
    - var random = Math.floor(Math.random()* 5)
    - var selected_image = IMG[random]
    img(src=`/images/${selected_image}`, `alt=Random Image ${i+1}`, height="300", width="300")